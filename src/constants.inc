;;
; @brief This file contains constants
;;

;;
; @def LED pin with pwm output
;;

LED     equ 0x02


;;
; @def MAX maximum value of the pointer
;;

MAX     equ 0xFE


;;
; @def SEND this byte cause the uart to send
;;

SEND    equ 'S'


;;
; @def WAIT this byte disables the sending of the uart
;;

WAIT    equ 'W'

